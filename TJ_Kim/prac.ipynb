    "def timeConversion(s):\n",
    "    hour = int(s[:2])\n",
    "    min_sec = s[2:8]\n",
    "    if s[-2:] == 'AM':\n",
    "        hour = hour % 12\n",
    "    else :\n",
    "        hour = hour % 12 + 12\n",
    "\n",
    "    return f\"{hour:02}{min_sec}\""
   ]
